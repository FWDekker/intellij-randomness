        <properties>
          <name value="fixedLengthEnabled"/>
          <text resource-bundle="randomness" key="settings.fixed_length"/>
        </properties>
      </component>
      <component id="bd193" class="com.fwdekker.randomness.ui.JIntSpinner" binding="lengthInput" custom-create="true">
